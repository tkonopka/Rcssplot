/**
 * Rcss for Rcssplot vignette, barplot part 2
 */

 axis {
  cex.axis: 1.2; lwd: 1.2;
}
axis.x {
  col.axis: #111111; line: -0.35; lwd: 0;
}
mtext.ylab, mtext.submain, axis.y {
  col: #444444;
}
axis.y {
  col.axis: #444444; col.ticks: #444444; las: 1; lwd.ticks: 1.2; tck: -0.03;
}
mtext {
  cex: 0.8; adj: 0;
}
mtext.main {
  line: 2.2; cex: 1.1;
}
mtext.ylab {
  line: 3; adj: 0.5;
}
mtext.submain {
  line: 0.9;
}
