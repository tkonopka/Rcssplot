/**
 * A style used during testing defaults/overloading
 */


barplot {
  col: #000080;
  border: NA;
}
barplot.abc {
  col: #9944dd;
}
axis {
  cex.axis: 1.2;
  lwd: 1.2;
}
axis.x {
  line: -0.35;
  lwd: 0;
}
axis.y {
  las: 1;
  tck: -0.03;
  lwd.ticks: 1.2;
}
mtext {
  line: 2.2;
  cex: 1.1;
  adj: 0;
}
mtext.submain {
  line: 0.7;
  col: #777777;
  cex: 1;
}
mtext.right {
  adj: 1
}

